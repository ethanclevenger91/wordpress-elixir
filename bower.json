{
  "name": "WordpressGulpBowerStarter",
  "version": "1.0.0",
  "authors": [
    "Ethan <ethan@webspecdesign.com>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap-sass-official": "^3.3.3",
    "fontawesome": ">=4.3.0",
    "html5shiv": ">=3.7.2",
    "respond": ">=1.4.2"
  },
  "overrides": {
    "fontawesome": {
      "main": [
        "./fonts/*",
        "scss/font-awesome.scss"
      ]
    }
  },
  "main": [
    "js/**/*.js",
    "sass/main.scss",
    "sass/fonts/**/*"
  ]
}
