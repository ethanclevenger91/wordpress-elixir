{
  "bsProxy": "http://replaceme.com",
  "paths": {
    "root":"../..",
  	"scripts": "../../js/**/*.js",
  	"styles": "../../sass/**/*.scss",
  	"fonts": "../../sass/fonts/**/*",
  	"images": "../../img/**/*.{png,jpg,jpeg,gif,svg}",
  	"bowerDir": "../../bower_components",
    "bowerJson": "../../bower.json"
  },
  "destPaths": {
    "build": "../../build",
  	"scripts": "../../build/js",
  	"styles": "../../build/css",
  	"fonts": "../../build/fonts",
  	"images": "../../build/img"
  },
  "disableNotifications": false
}
